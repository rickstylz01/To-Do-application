<div class="todo todo-edit">
    <h1>Edit Task <%= todo._id %></h1>
    <form action="/todos/<%=todo._id%>" class="d-flex" method="POST">

        <input type="text" name="task" class="form-control" value="<%= todo.task %>">
        <input type="submit" value="Update" class="btn btn-md btn-primary mx-3">
    </form>
</div>
